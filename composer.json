{
    "name": "vectorface/mysqlite",
    "description": "Select MySQL compatibility functions for PDO's SQLite driver",
    "keywords": ["SQLite", "MySQL", "debug", "testing", "compatibility"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vectorface\\MySQLite\\": "src/Vectorface/MySQLite/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vectorface\\Tests\\MySQLite\\": "tests/Vectorface/Tests/MySQLite/"
        }
    },
    "homepage": "https://github.com/Vectorface/MySQLite",
    "support": {
        "issues": "https://github.com/Vectorface/MySQLite/issues",
        "source": "https://github.com/Vectorface/MySQLite"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.0",
        "codeclimate/php-test-reporter": "dev-master"
    }
}
